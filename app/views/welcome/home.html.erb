
<div class="container home-page_center" >
  <div class="row vertical-align">
   <div class="col-md-6">
         <br>
         <br>
         <%= button_to 'Create New Wishlist', new_user_wishlist_path(current_user), method: :get, class: "btn btn-primary btn-lg" %>
   </div>
   <div class="col-md-6">
         <br>
         <br>
         <%= form_tag(users_path, :method => "get", id: "search-form") do %>
             <%= text_field_tag :search, params[:search], placeholder: "Enter User Name" %>
             <%= submit_tag "Find Users", :name => nil, class: "btn btn-primary btn-lg" %>
         <% end %>
   </div>
  </div>
 </div>
