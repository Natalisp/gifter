<%= form_for [current_user, Wishlist.new] do |f| %>

  <p>Title: <%= f.text_field :name %></p>
  <p>
  Add Gifts
    <%= f.fields_for :gift_attributes do |gift| %>
  </p>
  <p>
    What: <%= gift.text_field :name %>
  </p>
  <p>
    From where: <%= gift.text_field :link %>
</p>
<% end %>
  <%= f.submit %>
<% end %>
